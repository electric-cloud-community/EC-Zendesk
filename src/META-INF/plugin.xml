<?xml version="1.0" encoding="UTF-8"?>
<plugin>
  <key>EC-Zendesk</key>
  <version>1.0.0.29</version>
  <label>EC-Zendesk</label>
  <description>Integrates Zendesk ticketing system into Electric Commander.</description>
  <help>help.xml</help>
  <author>L. Rochette</author>
  <authorUrl>mailto:lrochette@electric-cloud.com</authorUrl>

  <commander-version min="4.1"/>

  <configure>configurations.xml</configure>
  <components>
      <component name="ConfigurationManagement">
          <javascript>war/ecplugins.EC-Zendesk.ConfigurationManagement/ecplugins.EC-Zendesk.ConfigurationManagement.noc
ache.js</javascript>
      </component>
  </components>
    <!-- Specify a category for this plugin; this is used in filtered lists in the Plugin Manager.
         Typically, it is set to System or Step.
     -->
  <category>Administration</category>
  <depends>EC-Core</depends>
</plugin>
